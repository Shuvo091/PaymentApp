@{
    ViewData["Title"] = "Payment Result";
}

<h2>Payment Result</h2>

<div>
    @if (ViewBag.PaymentResult != null)
    {
        <p>Payment Status: @ViewBag.PaymentResult</p>
        <!-- Display additional payment details or messages based on the payment result -->
    }
    else
    {
        <p>No payment result available.</p>
    }

    <!-- Add additional HTML/CSS to enhance the display of payment result -->
</div>
